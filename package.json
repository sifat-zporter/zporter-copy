{
  "name": "firebase-function",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:dev": "NODE_ENV=dev nest build",
    "build:stg": "NODE_ENV=stg nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:stg": "NODE_ENV=stg nest start --watch",
    "start:prod": "NODE_ENV=prod nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy:dev": "NODE_ENV=dev firebase deploy --only functions -P dev",
    "deploy:stg": "NODE_ENV=stg firebase deploy --only functions -P stg",
    "deploy:dev:api": "NODE_ENV=dev firebase deploy --only functions:api -P dev",
    "deploy:stg:api": "NODE_ENV=stg firebase deploy --only functions:api -P stg",
    "create-env": "printenv > .env",
    "build:docker": "docker build -t zporter/zporter-api:latest -f Dockerfile.local ."
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.13.0",
    "@google-cloud/bigquery": "^5.10.0",
    "@google/genai": "^1.8.0",
    "@google/generative-ai": "^0.24.1",
    "@hubspot/api-client": "^6.0.1-beta3",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/mapped-types": "^0.4.1",
    "@nestjs/mongoose": "9.0.3",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.8.2",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "^7.1.5",
    "@sendgrid/mail": "^7.4.4",
    "@types/chalk": "^2.2.0",
    "@types/newrelic": "^7.0.2",
    "axios": "^0.21.4",
    "cache-manager": "^3.6.0",
    "chalk": "^4.1.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "firebase": "^9.6.3",
    "firebase-admin": "^11.0.0",
    "firebase-functions": "^3.21.0",
    "fs-extra": "^9.1.0",
    "geoip-lite": "^1.4.3",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "mongoose": "6.2.2",
    "newrelic": "^8.3.0",
    "node-fetch": "^2.6.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "stripe": "^11.10.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/babel__traverse": "7.17.1",
    "@types/cache-manager": "^3.4.3",
    "@types/cron": "^1.7.3",
    "@types/estree": "^1.0.7",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.3.4",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.2.3"
  },
  "main": "dist/index.js",
  "engines": {
    "node": "12"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "volta": {
    "node": "14.18.3"
  }
}
